<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wddemo.dao.adminMapper">
    <select id="selectone" parameterType="String" resultType="wddemo.pojo.adminuser">
        select * from adminuser where admname=#{name} and admpass=#{pass}
    </select>
</mapper>